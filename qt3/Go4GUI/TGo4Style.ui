<connections>
    <connection>
        <sender>Palette</sender>
        <signal>valueChanged(int)</signal>
        <receiver>TGo4Style</receiver>
        <slot>SetPalette(int)</slot>
    </connection>
    <connection>
        <sender>PadColor</sender>
        <signal>clicked()</signal>
        <receiver>TGo4Style</receiver>
        <slot>SetPadColor()</slot>
    </connection>
</connections>
<includes>
