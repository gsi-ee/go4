    <connection>
        <sender>buttonOk</sender>
        <signal>clicked()</signal>
        <receiver>TGo4OptStatsSettings</receiver>
        <slot>accept()</slot>
    </connection>
    <connection>
        <sender>buttonCancel</sender>
        <signal>clicked()</signal>
        <receiver>TGo4OptStatsSettings</receiver>
        <slot>reject()</slot>
    </connection>
    <connection>
        <sender>buttonOk</sender>
        <signal>clicked()</signal>
        <receiver>TGo4OptStatsSettings</receiver>
        <slot>setFlags()</slot>
    </connection>
    <connection>
        <sender>DrawStatboxButton</sender>
        <signal>toggled(bool)</signal>
        <receiver>OptStatGroupBox</receiver>
        <slot>setEnabled(bool)</slot>
    </connection>
</connections>
<includes>
