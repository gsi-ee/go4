    <include location="local" impldecl="in implementation">TGo4Marker.h</include>
    <include location="local" impldecl="in implementation">TGo4QSettings.h</include>
    <include location="local" impldecl="in implementation">TGo4OptStatsSettings.ui.h</include>
</includes>
