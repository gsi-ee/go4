:: detect path where script is located 

call @ROOT_BINDIR@/thisroot.bat

@set OLDPATH=%cd%
@set THIS=%0
@set THIS=%THIS:~0,-12%.
@cd /D %THIS%
@set GO4SYS=%cd%
@cd /D %OLDPATH%

@echo Set environment for root and go4
@echo    GO4SYS = %GO4SYS%
@echo    ROOTSYS = %ROOTSYS%
@set PATH=%GO4SYS%\bin;%PATH%
@set LIB=.;%GO4SYS%\lib;%LIB%


